{
  "name": "@cdf/cvra",
  "version": "0.0.1",
  "description": "CVRA Microservices",
  "config": {
    "environment": "development"
  },
  "typings": "dist/index.d.ts",
  "scripts": {
    "clean": "rm -rf dist build tsconfig.tsbuildinfo",
    "reset": "rm -rf cli node_modules package-lock.json",
    "lint": "tslint \"src/**/*.ts\"",
    "build": "npx tsc -b && npx shx cp -R 'src/config' 'dist/'",
    "test": "rushx lint && NODE_ENV= NODE_CONFIG_DIR='./src/config' jest --silent",
    "start": "rushx build && NODE_CONFIG=$(cat ${CONFIG_LOCATION}/cvra/${npm_package_config_environment}-config.json) NODE_CONFIG_DIR='./src/config' ",
    "prebundle": "npx shx rm -rf ../../deploy/cvra bundle.zip && npx shx mkdir -p ../../deploy/cvra",
    "bundle": "rush deploy --overwrite --project @cdf/cvra --target-folder ../../deploy/cvra",
    "postbundle": "cd ../../deploy/cvra && zip --symlinks -rq ../../packages/cvra/bundle.zip * && cd -"
  },
  "dependencies": {
    "@cdf/errors": "2.1.0",
    "@cdf/logger": "2.1.0",
    "@cdf/lambda-invoke": "1.1.1",
    "@cdf/assetlibrary-client": "3.3.5",
    "@cdf/config-inject": "2.1.0",
    "aws-sdk": "2.804.0",
    "cfn-response-promise": "1.0.1",
    "config": "3.3.6",
    "inversify": "5.1.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "jest": "24.9.0",
    "js-yaml": "3.13.1",
    "logform": "2.1.2",
    "moment": "2.24.0",
    "ow": "0.15.0",
    "readable-stream": "2.3.6",
    "reflect-metadata": "0.1.13",
    "shortid": "2.2.15",
    "superagent": "7.0.2",
    "@types/config": "0.0.38",
    "@types/jest": "24.9.0",
    "@types/node": "14.6.1",
    "@types/shortid": "0.0.29",
    "@types/superagent": "4.1.10",
    "@types/underscore": "^1.9.4",
    "underscore": "1.9.2",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  }
}
