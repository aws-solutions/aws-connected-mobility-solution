{
  "name": "@cdf/greengrass-provisioning-client",
  "version": "1.0.1",
  "description": "CDF Greengrass Provisioning Client",
  "author": "Dean Hart",
  "main": "dist/index.js",
  "types": "dist/index.d.js",
  "scripts": {
    "clean": "npx shx rm -rf dist tsconfig.tsbuildinfo bundle.zip .rush .nyc_output *.log",
    "lint": "npx eslint . --ext '.ts'",
    "build": "npx tsc -b",
    "test": "rushx lint && NODE_ENV= NODE_CONFIG_DIR='./src/config' jest --silent --passWithNoTests"
  },
  "dependencies": {
    "@cdf/lambda-invoke": "1.1.1",
    "aws-sdk": "2.804.0",
    "config": "3.3.6",
    "inversify": "5.1.1",
    "inversify-binding-decorators": "4.0.0",
    "inversify-express-utils": "6.3.2",
    "js-yaml": "3.13.1",
    "logform": "2.1.2",
    "ow": "0.15.0",
    "readable-stream": "2.3.6",
    "reflect-metadata": "0.1.13",
    "superagent": "7.0.2",
    "winston": "3.2.1",
    "winston-transport": "4.3.0"
  },
  "devDependencies": {
    "@types/config": "0.0.38",
    "@types/node": "14.6.1",
    "@types/superagent": "4.1.10",
    "eslint": "^6.1.0",
    "nyc": "15.1.0",
    "shx": "0.3.3",
    "@types/jest": "24.9.0",
    "@typescript-eslint/eslint-plugin": "4.23.0",
    "@typescript-eslint/parser": "4.23.0",
    "jest-create-mock-instance": "1.1.0",
    "jest-haste-map": "24.9.0",
    "jest-mock-extended": "1.0.14",
    "jest-mock": "24.9.0",
    "jest-resolve": "24.9.0",
    "jest": "24.9.0",
    "ts-jest": "24.2.0",
    "typescript": "4.2.4"
  },
  "jest": {
    "globals": {
      "NODE_CONFIG_STRICT_MODE": false,
      "SUPPRESS_NO_CONFIG_WARNING": true
    },
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "<rootDir>/**/?(*.)+(spec|test).ts?(x)"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "license": "ISC",
  "private": true
}
